{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/src/swagger-ui/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}